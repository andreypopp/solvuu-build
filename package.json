{
    "name": "solvuu-build",
    "exportedEnvVars": {
        "CAML_LD_LIBRARY_PATH": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/lib/stublibs"
        },
        "solvuu_build_version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "0.1.0"
        },
        "FINDLIB": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/lib"
        },
        "version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "0.1.0"
        },
        "solvuu_build_enable": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "enable"
        },
        "solvuu_build_installed": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "true"
        }
    },
    "version": "0.1.0",
    "dependencies": {
        "ocamlfind": "andreypopp/ocamlfind",
        "ocamlgraph": "andreypopp/ocamlgraph#pjc",
        "opam-installer-bin": "andreypopp/opam-installer-bin"
    },
    "pjc": {
        "build": "make byte && make native && make _build/META && make solvuu-build.install && opam-installer --prefix=$cur__install"
    },
    "peerDependencies": {
        "ocaml": "andreypopp/ocaml#trunk"
    }
}
